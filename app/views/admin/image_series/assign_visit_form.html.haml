= semantic_form_for @image_series, :url => assign_visit_admin_image_series_path(@image_series, :return_url => @return_url), :method => :post do |f|  
  = f.inputs 'Assign Visit' do
    = f.input :visit, :collection => @image_series.patient.visits

  = f.actions do
    = f.action :submit, :as => :input
    = f.action :cancel, :as => :link, :wrapper_html => {:class => 'cancel'}, :url => @return_url