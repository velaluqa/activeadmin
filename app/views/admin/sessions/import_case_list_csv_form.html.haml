= semantic_form_for @session, :url => import_case_list_csv_admin_session_path(@session), :method => :post do |f|
  = f.inputs 'Import' do
    = f.input :file, :label => 'CSV File', :as => :file
    = f.input :annotations_layout_mode, :label => 'Case Mode', :as => :radio, :collection => {'Regular' => :regular, 'Validation' => :validation}, :required => true

  = f.actions do
    = f.action :submit, :as => :input
    = f.action :cancel, :as => :link, :wrapper_html => {:class => 'cancel'}