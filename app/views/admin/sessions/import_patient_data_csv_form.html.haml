= semantic_form_for @session, :url => import_patient_data_csv_admin_session_path(@session), :method => :post do |f|
  = f.inputs 'Import' do
    = f.input :file, :label => 'CSV File', :as => :file

  = f.actions do
    = f.action :submit, :as => :input
    = f.action :cancel, :as => :link, :wrapper_html => {:class => 'cancel'}