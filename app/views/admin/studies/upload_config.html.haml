= semantic_form_for @study, :url => url, :method => :post do |f|
  = f.inputs 'Upload Configuration' do
    = f.input :file, :label => 'Configuration File', :as => :file

  = f.actions do
    = f.action :submit, :as => :input
    = f.action :cancel, :as => :link, :wrapper_html => {:class => 'cancel'}
