= semantic_form_for @form, :url => url, :method => :post do |f|
  = f.inputs 'Select session to copy to' do
    = f.input :session, :collection => current_user.sessions, :include_blank => false

  = f.actions do
    = f.action :submit, :as => :input
    = f.action :cancel, :as => :link, :wrapper_html => {:class => 'cancel'}